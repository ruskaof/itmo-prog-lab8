<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainBorderPane" prefHeight="333.0" prefWidth="524.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="com.ruskaof.client.presentation.controllers.MainScreenController">
    <left>
        <VBox spacing="5">
            <Button onAction="#navigateToTableView" text="btn 1"/>
            <Button onAction="#navigateToVisualisation" text="btn 2"/>
        </VBox>
    </left>
    <center>
        <VBox spacing="5" alignment="CENTER">
            <fx:include fx:id="tableView" source="screen_table_view.fxml"/>

            <fx:include fx:id="visualisation" source="screen_visualisation.fxml" managed="false" visible="false"/>
        </VBox>
    </center>
</BorderPane>
